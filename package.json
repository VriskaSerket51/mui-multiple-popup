    "@mui/material": "^5.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.2.0",
